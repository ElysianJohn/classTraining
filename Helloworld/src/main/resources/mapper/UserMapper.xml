<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo2.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.example.demo2.entity.User">
            <id property="userId" column="userId" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="email" column="email" jdbcType="VARCHAR"/>
            <result property="institute" column="institute" jdbcType="VARCHAR"/>
            <result property="insert_id" column="insert_id" jdbcType="VARCHAR"/>
            <result property="insert_ts" column="insert_ts" jdbcType="TIMESTAMP"/>
            <result property="update_id" column="update_id" jdbcType="VARCHAR"/>
            <result property="update_ts" column="update_ts" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        userId,name,email,
        institute,insert_id,insert_ts,
        update_id,update_ts
    </sql>
</mapper>
